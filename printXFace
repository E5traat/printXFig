coordinates = [[2,2], [3,3], [2,4], [2,6], [3,6], [2,7], [3,7],[7,4], [7,5], [6,3], [6,6]]
def print_face(coordinates, size = 10):
    for i in range(size):
      row_text = ""
      for j in range(size):
        if [i,j] in coordinates:
          row_text += "  "
        else:
          row_text += "X "
      print(row_text)
print_face(coordinates)
